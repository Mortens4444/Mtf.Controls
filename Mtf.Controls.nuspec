<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Mtf.Controls</id>
    <version>1.1.12</version>
    <description>
      Custom Windows Forms Controls.
      
      The library includes various controls, such as:
      - AnsiColoringRichTextBox
      - EditableComboBox
      - FileBrowserView
      - Kbd300ASimulator
      - MovablePanel
      - MovableSizablePanel
      - MtfListView
      - MtfPictureBox
      - MtfTreeView
      - PasswordBox
      - RichTextBoxWithLineNumbers
      - RotatableImagePanel
      - SourceCodeViewerRichTextBox
      - TextBoxWithRegEx
      - TransparentPanel
      
      Compatibility:
      - .NET Framework 4.5.2
      - .NET Framework 4.6.2
      - .NET Framework 4.8.1
      - .NET 8
      - .NET 9
    </description>
    <authors>Mortens</authors>
    <owners>Mortens</owners>
    <copyright>Copyright © 2025</copyright>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/Mortens4444/Mtf.Controls</projectUrl>
    <repository type="git" url="https://github.com/Mortens4444/Mtf.Controls.git" branch="main" />
    <tags>AnsiColoringRichTextBox EditableComboBox FileBrowserView Kbd300ASimulator MovablePanel MovableSizablePanel MtfListView MtfPictureBox MtfTreeView PasswordBox RichTextBoxWithLineNumbers RotatableImagePanel SourceCodeViewerRichTextBox TextBoxWithRegEx TransparentPanel Mortens</tags>
    <readme>docs\ReadMe.md</readme>
    <dependencies>
      <group targetFramework="net452">
        <dependency id="Mtf.MessageBoxes" version="6.0.2" />
        <dependency id="System.Net.Http" version="4.3.4" />
      </group>
      <group targetFramework="net462">
        <dependency id="Mtf.MessageBoxes" version="6.0.2" />
        <dependency id="System.Net.Http" version="4.3.4" />
      </group>
      <group targetFramework="net481">
        <dependency id="Mtf.MessageBoxes" version="6.0.2" />
        <dependency id="System.Net.Http" version="4.3.4" />
      </group>
      <group targetFramework="net8.0-windows7">
        <dependency id="Mtf.MessageBoxes" version="6.0.2" />
      </group>
      <group targetFramework="net9.0-windows7">
        <dependency id="Mtf.MessageBoxes" version="6.0.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src=".\Mtf.Controls\bin\Release\net452\**" target="lib\net452" />
